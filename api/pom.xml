    <groupId>org.jenkinsmvn</groupId>
    <version>1.0-SNAPSHOT</version>
    <relativePath>../pom.xml</relativePath>
  </parent>

  <groupId>org.jenkinsmvn</groupId>
  <artifactId>jenkins-api</artifactId>
  <version>1.0-SNAPSHOT</version>

  <packaging>jar</packaging>
  <name>jenkins-api</name>

  <build>
    <defaultGoal>install</defaultGoal>
    <plugins>
